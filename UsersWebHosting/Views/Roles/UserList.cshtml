@model IEnumerable<UsersWebHosting.Data.Models.User>

<h2>List of users</h2>
<table class="table">
    @foreach (var user in Model)
    {
            <tr>
                <td>@user.Email</td>
                <td>
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Change permissions</a>
                </td>
            </tr>
    }
</table>